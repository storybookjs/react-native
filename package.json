    "babel-runtime": "^6.5.0",
    "deep-equal": "^1.0.1",
    "fuzzysearch": "^1.0.3",
    "json-stringify-safe": "^5.0.1",
    "keycode": "^2.1.1",
    "lodash.pick": "^4.2.1",
    "mantra-core": "^1.6.1",
    "qs": "^6.2.0",
    "react-modal": "^1.2.1"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
