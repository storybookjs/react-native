  "devDependencies": {
    "@storybook/types": "^7",
    "@types/jest": "^29.4.3",
    "@types/react": "~18.2.14",
    "babel-jest": "^29.4.3",
    "jest": "^29.4.3",
    "jotai": "^2.0.2",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1",
    "react": "*",
    "react-native": ">=0.57.0",
    "react-native-safe-area-context": "*"
  },
  "engines": {
    "node": ">=8.0.0"
