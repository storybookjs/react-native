    "directory": "packages/react-native"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "sb-rn-get-stories": "./bin/get-stories.js",
    "sb-rn-watcher": "./bin/watcher.js"
  },
  "files": [
    "bin/**/*",
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "scripts/*",
    "template/**/*"
