    "@storybook/addon-options": "6.0.0-alpha.0",
    "@storybook/addon-storysource": "6.0.0-alpha.0",
    "@storybook/addons": "6.0.0-alpha.0",
    "@storybook/marko": "6.0.0-alpha.0",
    "@storybook/source-loader": "6.0.0-alpha.0",
    "prettier": "^1.16.4",
    "webpack": "^4.33.0"
  }
