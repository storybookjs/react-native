{
  "name": "preact-example",
  "version": "5.1.0-alpha.3",
  "private": true,
  "scripts": {
    "now-build": "node ../../scripts/bootstrap --core && yarn run build-storybook --quiet",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-storybook": "build-storybook -s public",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "dependencies": {
    "global": "^4.3.2",
    "preact": "^8.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@storybook/addon-actions": "5.1.0-alpha.3",
    "@storybook/addon-backgrounds": "5.1.0-alpha.3",
    "@storybook/addon-centered": "5.1.0-alpha.3",
    "@storybook/addon-knobs": "5.1.0-alpha.3",
    "@storybook/addon-links": "5.1.0-alpha.3",
    "@storybook/addon-notes": "5.1.0-alpha.3",
    "@storybook/addon-options": "5.1.0-alpha.3",
    "@storybook/addon-storyshots": "5.1.0-alpha.3",
    "@storybook/addon-storysource": "5.1.0-alpha.3",
    "@storybook/addon-viewport": "5.1.0-alpha.3",
    "@storybook/addons": "5.1.0-alpha.3",
    "@storybook/preact": "5.1.0-alpha.3",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "file-loader": "^3.0.1",
    "preact-render-to-json": "^3.6.6",
    "raw-loader": "^1.0.0",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1"
  }
}
